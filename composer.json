{
  "name": "fond-of-spryker/jellyfish-b2b",
  "description": "Jellyfish B2B module.",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Rose",
      "email": "daniel-rose@gmx.de"
    }
  ],
  "scripts": {
    "ci": "make ci",
    "codeception": "make codeception",
    "phpcs": "make phpcs",
    "phpstan": "make phpstan"
  },
  "require": {
    "php": ">=7.3",
    "fond-of-spryker/jellyfish-b2b-extension": "dev-master",
    "spryker/company": "^1.0.0",
    "spryker/company-business-unit": "^1.0.0 || ^2.0.0",
    "spryker/company-unit-address": "^1.0.0",
    "spryker/event": "^2.0.0",
    "spryker/event-behavior": "^1.0.0",
    "spryker/locale": "^2.2.0 || ^3.0.0 || ^4.0.0",
    "spryker/currency": "^3.0.0 || ^4.0.0",
    "spryker/util-encoding": "^1.0.0 || ^2.0.0"
  },
  "require-dev": {
    "fond-of-codeception/spryker": "*",
    "spryker/code-sniffer": "*",
    "spryker-sdk/phpstan-spryker": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfSpryker\\": "src/FondOfSpryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfSpryker\\": "tests/FondOfSpryker/",
      "Generated\\": "src/Generated/",
      "Orm\\Zed\\": "src/Orm/Zed/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
